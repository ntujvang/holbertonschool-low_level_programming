#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/**
 * main working area of our program
 *
 * Description: selects last digit on a set of #
 * and displays it's position according to 0, 5 and 6
 *
 * Return: 0 to end it
 */
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	/* your code goes there */
	if (n % 10  < 6)
	{
		printf("Last digit of %d is %d and is less than 6 and not 0\n",
		       n, n % 10);
	}
	else if (n % 10 == 0)
	{
		printf("Last digit of %d is %d and is 0\n", n, n % 10);
	}
	else
		printf("Last digit of %d is %d and is greater than 5\n", n,
			n % 10);
	return (0);
}
